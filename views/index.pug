extends layout.pug

block content
  section#video
    h2 Let get the camera
    button#capturewebcam Start Webcam
    video(height="480px" width="640px" id="webcamfeed" autoplay controls)

  section#data
    h2 Sphero's data
    form(action='/', method='post')
      button(type='submit') Get the ball's data
    p= data